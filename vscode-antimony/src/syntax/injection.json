{
    "scopeName": "todo-comment.injection",
    "injectionSelector": [
        "L:comment.line.double-slash", 
        "comment.line.number-sign.antimony",
        "comment.block.antimony"
    ],
    "patterns": [
      {
        "include": "#todo-keyword"
      }
    ],
    "repository": {
      "todo-keyword": {
        "match": "TODO:",
        "name": "keyword.todo"
      }
    }
  }